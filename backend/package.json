    "license": "MIT",
    "dependencies": {
        "@types/multer": "^2.0.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "csv-parse": "^6.1.0",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.9.3",
        "multer": "^2.0.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^22.10.2",
        "@types/supertest": "^6.0.2",
        "jest": "^29.7.0",
        "supertest": "^7.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.2"
